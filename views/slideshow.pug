extends layout

block content
  div(class='slideshow')
    div(class='slideshow__content')
      div(class='slideshow__item slideshow__item--active')
        img(src='images/entrance_desktop.png')
      div(class='slideshow__item')
        img(src='images/entrance_mobile.png')
    div(class='slideshow__buttons')
      button(class='slideshow__button slideshow__button--active')
        | 1
      button(class='slideshow__button')
        | 2
